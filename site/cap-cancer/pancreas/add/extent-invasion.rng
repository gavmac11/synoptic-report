<?xml version="1.0" encoding="UTF-8"?>
<element datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes"
    name="invasion" ns="http://www.cap.org/pert/2009/01/pancreas/"
    xmlns="http://relaxng.org/ns/structure/1.0"
    xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">
    <interleave>
        <ref name="baseContent"/>
        <zeroOrMore>
            <element name="invasionFinding">
                <ref name="baseContent"/>
                <attribute name="location">
                    <choice>
                        <value>invades ampulla of Vater</value>
                        <value>invades sphincter of Oddi</value>
                        <value>invades duodenal wall</value>
                        <value>invades peripancreatic soft tissue</value>
                        <value>invades retroperitoneal soft tissue</value>
                        <value>invades mesenteric adipose tissue</value>
                        <value>invades mesocolon</value>
                        <value>invades extrapancreatic common bile duct</value>
                    </choice>
                </attribute>
                <attribute name="value">
                    <externalRef href="../../../../common/extendedBoolean.rng"/>
                </attribute>
                <zeroOrMore>
                    <element name="adjacentStructure"
                        ns="http://www.cap.org/pert/2009/01/pancreas/">
                        <ref name=" baseContent"/>
                        <attribute name="value"/>
                        <optional>
                            <attribute name="extent">
                                <choice>
                                    <value>adheres</value>
                                    <value>invades</value>
                                    <data type="string"></data>
                                </choice>
                            </attribute>
                        </optional>
                    </element>
                </zeroOrMore>
            </element>
        </zeroOrMore>
    </interleave>
</element>
