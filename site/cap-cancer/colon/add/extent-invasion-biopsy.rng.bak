<?xml version="1.0" encoding="UTF-8"?>
<element name="invasion" ns="http://www.cap.org/pert/2009/01/" xmlns="http://relaxng.org/ns/structure/1.0"
    xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">
    <interleave>
        <ref name="base.standard.content"/>
        <element name="deepestInvasion" ns="http://www.cap.org/pert/2009/01/colon/">
            <ref name="base.standard.content"/>
            <optional><element name="value">
                <choice>
                    <value>no evidence of tumor</value>
                    <value>intraepithelial carcinoma</value>
                    <value>lamina propria</value>
                    <value>submucosa</value>
                    <value>muscularis propria</value>
                    <value>inapplicable</value>
                    <value>inconclusive</value>
                    <value>indeterminate</value>
                    <value>other</value>
                </choice>
            </element></optional>
        </element>
    </interleave>
</element>
