      </define>
   </div>
   <div>
      <a:documentation>============================================</a:documentation>
      <a:documentation>== studies SECTION VALUES ==</a:documentation>
      <define
         combine="choice" name="studies.study.name.values">
         <value>specify</value>
      </define>
      <define combine="choice" name="studies.study.value.values">
         <value>specify</value>
      </define>
      <define combine="choice" name="studies.study.method.values">
