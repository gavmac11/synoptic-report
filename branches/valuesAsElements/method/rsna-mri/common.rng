<grammar datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes" ns="http://www.rsna.org/reporting/2009/01/"
    xmlns="http://relaxng.org/ns/structure/1.0" xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">
    <define combine="interleave" name="comparisons">
        <element name="comparisons">
            <ref name="comparisons.attrib"/>
            <interleave>
                <ref name="standard.content"/>
                <ref name="comparisons.content"/>
            </interleave>
        </element>
    </define>
    <define combine="interleave" name="indications">
        <element name="indications">
            <ref name="indications.attrib"/>
            <interleave>
                <ref name="standard.content"/>
                <ref name="indications.content"/>
            </interleave>
        </element>
    </define>
    <define combine="interleave" name="techniques">
        <element name="techniques">
            <ref name="techniques.attrib"/>
            <interleave>
                <ref name="standard.content"/>
                <ref name="techniques.content"/>
            </interleave>
        </element>
    </define>
    <define combine="interleave" name="findings">
        <element name="findings">
            <ref name="findings.attrib"/>
            <interleave>
                <ref name="standard.content"/>
                <ref name="findings.content"/>
            </interleave>
        </element>
    </define>
    <define combine="interleave" name="impressions">
        <element name="impressions">
            <ref name="impressions.attrib"/>
            <interleave>
                <ref name="standard.content"/>
                <ref name="impressions.content"/>
            </interleave>
        </element>
    </define>
    <!--  -->
    <define combine="interleave" name="comparisons.attrib">
        <empty/>
    </define>
    <define combine="interleave" name="comparisons.content">
        <empty/>
    </define>
    <define combine="interleave" name="indications.attrib">
        <empty/>
    </define>
    <define combine="interleave" name="indications.content">
        <empty/>
    </define>
    <define combine="interleave" name="techniques.attrib">
        <empty/>
    </define>
    <define combine="interleave" name="techniques.content">
        <empty/>
    </define>
    <define combine="interleave" name="findings.attrib">
        <empty/>
    </define>
    <define combine="interleave" name="findings.content">
        <empty/>
    </define>
    <define combine="interleave" name="impressions.attrib">
        <empty/>
    </define>
    <define combine="interleave" name="impressions.content">
        <empty/>
    </define>
</grammar>
