<grammar xmlns="http://relaxng.org/ns/structure/1.0">
  <define name="extendedBoolean">
    <choice>
      <value>positive</value>
      <value>negative</value>
      <value>equivocal</value>
      <value>not applicable</value>
    </choice>
  </define>
</grammar>
