<grammar datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes" ns="http://purl.org/pathology/ecc/"
    xmlns="http://relaxng.org/ns/structure/1.0" xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">
    <!--=============================================================-->
    <a:documentation>-- shared valuesets --</a:documentation>
    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <define name="site.values">
        <choice>
            <value>empty</value>
        </choice>
    </define>
    <!--=============================================================-->
    <a:documentation>-- ACCESSORY section valuesets --</a:documentation>
    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <define name="accessory.empty.values">
        <choice>
            <value>empty</value>
        </choice>
    </define>
    <!--=============================================================-->
    <a:documentation>== ADDITIONAL section valuesets ==</a:documentation>
    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <define name="additional.finding.values">
        <choice>
            <value>empty</value>
        </choice>
    </define>
    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <define name="additional.empty.values">
        <choice>
            <value>empty</value>
        </choice>
    </define>
    <!--=============================================================-->
    <a:documentation>-- HISTORY section valuesets --</a:documentation>
    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <define name="history.priorTherapy.values">
        <choice>
            <value>neoadjuvant chemotherapy</value>
            <value>neoadjuvant irradiation</value>
        </choice>
    </define>
    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <define name="history.empty.values">
        <choice>
            <value>empty</value>
        </choice>
    </define>
    <!--=============================================================-->
    <a:documentation>-- EXTENT section valuesets --</a:documentation>
    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <define name="extent.empty.values">
        <choice>
            <value>empty</value>
        </choice>
    </define>
    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <define name="extent.site.values">
        <choice>
            <ref name="specimen.site.values"/>
            <choice>
                <value>empty</value>
            </choice>
        </choice>
    </define>
    <!--=============================================================-->
    <a:documentation>-- MARGINS section valuesets --</a:documentation>
    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <define name="margins.margin.site.values">
        <choice>
            <value>empty</value>
        </choice>
    </define>
    <!--=============================================================-->
    <a:documentation>-- DISTANT section valuesets --</a:documentation>
    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <define name="distant.site.values">
        <value>empty</value>
    </define>
    <!--=============================================================-->
    <a:documentation>-- NODES section valuesets --</a:documentation>
    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <define name="nodes.nodeGroup.site.values">
        <choice>
            <value>empty</value>
        </choice>
    </define>
    <!--=============================================================-->
    <a:documentation>-- SPECIMEN section valuesets --</a:documentation>
    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <define name="specimen.procedure.values">
        <choice>
            <value>empty</value>
        </choice>
    </define>
    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <define name="specimen.site.values">
        <choice>
            <value>empty</value>
        </choice>
    </define>
    <!--=============================================================-->
    <a:documentation>-- STAGE section valuesets --</a:documentation>
    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <define name="stage.pT.values">
        <choice>
            <value>X</value>
            <value>0</value>
            <value>1</value>
            <value>2</value>
            <value>3</value>
            <value>4</value>
        </choice>
    </define>
    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <define name="stage.pN.values">
        <choice>
            <value>X</value>
            <value>0</value>
            <value>1</value>
        </choice>
    </define>
    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <define name="stage.pM.values">
        <choice>
            <value>0</value>
            <value>1</value>
        </choice>
    </define>
    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <define name="stage.group.values">
        <choice>
            <value>X</value>
            <value>I</value>
            <value>II</value>
            <value>III</value>
            <value>IV</value>
        </choice>
    </define>
    <!--=============================================================-->
    <a:documentation>-- STUDIES section valuesets --</a:documentation>
    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <define name="studies.empty.values">
        <choice>
            <value>empty</value>
        </choice>
    </define>
    <!--=============================================================-->
    <a:documentation>-- TUMOR section valuesets --</a:documentation>
    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <define name="tumor.type.values">
        <choice>
            <value>empty</value>
        </choice>
    </define>
    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <define name="tumor.gradeSystem.values">
        <choice>
            <value>empty</value>
        </choice>
    </define>
    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <define name="tumor.grade.values">
        <choice>
            <value>empty</value>
        </choice>
    </define>
    <!--=============================================================-->
</grammar>
