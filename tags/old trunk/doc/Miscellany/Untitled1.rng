                                            <nsName ns="http://www.rsna.org/reporting/2009/01/"/>
                                        </choice>
                                    </except>
                                </anyName>
                                <text/>
                            </attribute>
                        </zeroOrMore>
                        <interleave>
                            <zeroOrMore>
                                <ref name="displayText"/>
                            </zeroOrMore>
                            <zeroOrMore>
                                <ref name="comment"/>
                            </zeroOrMore>
                            <zeroOrMore>
                                <ref name="_1"/>
                            </zeroOrMore>
                            <optional>
                                <attribute name="otherValue" ns="">
                                    <text/>
                                </attribute>
                            </optional>
                            <text/>
                        </interleave>
                    </group>
                    <optional>
