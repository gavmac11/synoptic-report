            <value>is</value>
            <value>is (DCIS)</value>
            <value>is (LCIS)</value>
            <value>is (Paget)</value>
            <value>1</value>
            <value>1mi</value>
            <value>1a</value>
            <value>1b</value>
            <value>1c</value>
            <value>2</value>
            <value>3</value>
            <value>4</value>
            <value>4a</value>
            <value>4b</value>
            <value>4c</value>
            <value>4d</value>
        </choice>
    </define>
    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <define name="stage.pN.values">
        <choice>
            <value>X</value>
            <value>0</value>
            <value>0(i-)</value>
            <value>0(i+)</value>
            <value>0(mol-)</value>
            <value>0(mol+)</value>
            <value>1</value>
            <value>1mi</value>
            <value>1a</value>
            <value>1b</value>
            <value>1c</value>
            <value>2</value>
            <value>2a</value>
            <value>2b</value>
            <value>3</value>
            <value>3a</value>
            <value>3b</value>
            <value>3c</value>
        </choice>
    </define>
    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <define name="stage.pM.values">
        <choice>
            <value>0</value>
            <value>0(i+)</value>
            <value>1</value>
        </choice>
    </define>
    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <define name="stage.group.values">
        <choice>
            <value>0</value>
            <value>IA</value>
            <value>IB</value>
            <value>IIA</value>
            <value>IIB</value>
            <value>IIIA</value>
            <value>IIIB</value>
            <value>IIIC</value>
            <value>IV</value>
            <value>indeterminate</value>
        </choice>
    </define>
