            </element>
        </optional>
        <optional>
            <element name="peritumoralImmuneResponse">
                <grammar>
                    <include href="&selectOne;"/>
                    <define name="valueset">
                        <parentRef name="accessory.immuneResponse.valueset"/>
                    </define>
                </grammar>
            </element>
        </optional>
    </define>
