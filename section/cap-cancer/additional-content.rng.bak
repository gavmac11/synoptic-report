<grammar datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes" ns="http://www.cap.org/pert/2009/01/"
    xmlns="http://relaxng.org/ns/structure/1.0" xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">
    <define combine="interleave" name="additional.content">
        <optional>
            <element name="findings">
                <interleave>
                    <ref name="base.standard.content"/>
                    <oneOrMore>
                        <element name="finding">
                            <ref name="base.standard.content"/>
                            <optional>
                                <element name="value">
                                    <ref name="findings"/>
                                </element>
                            </optional>
                        </element>
                    </oneOrMore>
                </interleave>
            </element>
        </optional>
    </define>
    <define name="findings">
        <data type="string"/>
    </define>
</grammar>
