        <!--  -->
        <xs:group name="history.content">
            <xs:sequence>
                <xs:group ref="ecc:history.content"/>
                <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
                <!-- no site specific history items here -->
                <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
            </xs:sequence>
        </xs:group>
        <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
        <xs:group name="specimen.content">
            <xs:sequence>
                <xs:group ref="ecc:specimen.content"/>
                <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
                <!-- add site specific specimen items here (optional) -->
                <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
                <xs:element name="mesorectumIntact">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:group ref="ecc:generic.content"/>
                            <xs:element maxOccurs="1" minOccurs="0" name="response">
                                <xs:complexType>
                                    <xs:group ref="ecc:generic.response.content"/>
                                    <xs:attribute name="value" type="ecc:specimen.mesorectum.values"
                                        use="required"/>
                                    <xs:attributeGroup ref="ecc:generic.attrib"/>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                        <xs:attributeGroup ref="ecc:generic.attrib"/>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:group>
        <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
        <xs:group name="tumor.content">
            <xs:sequence>
                <xs:group ref="ecc:tumor.content"/>
                <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
                <!-- add site specific tumor items here (optional) -->
                <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
                <xs:element name="precursorPolyp">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:group ref="ecc:generic.content"/>
                            <xs:group ref="ecc:tumor.precursorPolyp.content"/>
                        </xs:sequence>
                        <xs:attributeGroup ref="ecc:generic.attrib"/>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:group>
        <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
        <xs:group name="extent.content">
            <xs:sequence>
                <xs:group ref="ecc:extent.content"/>
                <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
                <!-- add site specific extent items here (optional) -->
                <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
                <xs:element name="deepestInvasion">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:group ref="ecc:generic.content"/>
                            <xs:element maxOccurs="1" minOccurs="0" name="response">
                                <xs:complexType>
                                    <xs:group ref="ecc:generic.response.content"/>
                                    <xs:attribute name="value"
                                        type="ecc:extent.deepestInvasion.values" use="required"/>
                                    <xs:attributeGroup ref="ecc:generic.attrib"/>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                        <xs:attributeGroup ref="ecc:generic.attrib"/>
                    </xs:complexType>
                </xs:element>
                <xs:element name="tumorPerforation">
                    <xs:complexType>
                        <xs:group ref="ecc:qualitative.response"/>
                        <xs:attributeGroup ref="ecc:generic.attrib"/>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:group>
        <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
        <xs:group name="accessory.content">
            <xs:sequence>
                <xs:group ref="ecc:accessory.content"/>
                <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
                <!-- add site specific accessory items here (optional) -->
                <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
                <xs:element minOccurs="0" name="peritumoralDeposits">
                    <xs:complexType>
                        <xs:group ref="ecc:qualitative.response"/>
                        <xs:attributeGroup ref="ecc:generic.attrib"/>
                    </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="mucinousComponent">
                    <xs:complexType>
                        <xs:group ref="ecc:percent.response"/>
                        <xs:attributeGroup ref="ecc:generic.attrib"/>
                    </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="medullaryComponent">
                    <xs:complexType>
                        <xs:group ref="ecc:percent.response"/>
                        <xs:attributeGroup ref="ecc:generic.attrib"/>
                    </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="intratumoralImmuneResponse">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:group ref="ecc:generic.content"/>
                            <xs:element maxOccurs="1" minOccurs="0" name="response">
                                <xs:complexType>
                                    <xs:group ref="ecc:generic.response.content"/>
                                    <xs:attribute name="value"
                                        type="ecc:accessory.immuneResponse.values" use="required"/>
                                    <xs:attributeGroup ref="ecc:generic.attrib"/>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                        <xs:attributeGroup ref="ecc:generic.attrib"/>
                    </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="peritumoralImmuneResponse">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:group ref="ecc:generic.content"/>
                            <xs:element maxOccurs="1" minOccurs="0" name="response">
                                <xs:complexType>
                                    <xs:group ref="ecc:generic.response.content"/>
                                    <xs:attribute name="value"
                                        type="ecc:accessory.immuneResponse.values" use="required"/>
                                    <xs:attributeGroup ref="ecc:generic.attrib"/>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                        <xs:attributeGroup ref="ecc:generic.attrib"/>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:group>
        <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
        <xs:group name="margins.content">
            <xs:sequence>
                <xs:group ref="ecc:margins.content"/>
                <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
                <!-- add site specific items here (optional) -->
                <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
            </xs:sequence>
        </xs:group>
        <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
        <xs:group name="nodes.content">
            <xs:sequence>
                <xs:group ref="ecc:nodes.content"/>
                <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
                <!-- add site specific margins items here (optional) -->
                <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
            </xs:sequence>
        </xs:group>
        <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
        <xs:group name="distant.content">
            <xs:sequence>
                <xs:group ref="ecc:distant.content"/>
                <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
                <!-- add site specific distant items here (optional) -->
                <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
            </xs:sequence>
        </xs:group>
        <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
        <xs:group name="studies.content">
            <xs:sequence>
                <xs:group ref="ecc:studies.content"/>
                <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
                <!-- add site specific studies items here (optional) -->
                <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
                <xs:element minOccurs="0" name="microsatelliteInstability">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:group ref="ecc:generic.content"/>
                            <xs:group ref="ecc:studies.microsatelliteInstability.result"/>
                            <xs:group ref="ecc:studies.microsatelliteInstability.method"/>
                        </xs:sequence>
                        <xs:attributeGroup ref="ecc:generic.attrib"/>
                    </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="MLH1">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:group ref="ecc:generic.content"/>
                            <xs:group ref="ecc:studies.mismatchRepair.content"/>
                        </xs:sequence>
                        <xs:attributeGroup ref="ecc:generic.attrib"/>
                    </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="MSH">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:group ref="ecc:generic.content"/>
                            <xs:group ref="ecc:studies.mismatchRepair.content"/>
                        </xs:sequence>
                        <xs:attributeGroup ref="ecc:generic.attrib"/>
                    </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="MSH6">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:group ref="ecc:generic.content"/>
                            <xs:group ref="ecc:studies.mismatchRepair.content"/>
                        </xs:sequence>
                        <xs:attributeGroup ref="ecc:generic.attrib"/>
                    </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="PMS2">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:group ref="ecc:generic.content"/>
                            <xs:group ref="ecc:studies.mismatchRepair.content"/>
                        </xs:sequence>
                        <xs:attributeGroup ref="ecc:generic.attrib"/>
                    </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="BRAF">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:group ref="ecc:generic.content"/>
                            <xs:group ref="ecc:studies.mutationalAnalysis.result"/>
                            <xs:group ref="ecc:studies.BRAF.mutation"/>
                            <xs:group ref="ecc:studies.mutationalAnalysis.method"/>
                        </xs:sequence>
                        <xs:attributeGroup ref="ecc:generic.attrib"/>
                    </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="KRAS">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:group ref="ecc:generic.content"/>
                            <xs:group ref="ecc:studies.mutationalAnalysis.result"/>
                            <xs:group ref="ecc:studies.KRAS.mutation"/>
                            <xs:group ref="ecc:studies.mutationalAnalysis.method"/>
                        </xs:sequence>
                        <xs:attributeGroup ref="ecc:generic.attrib"/>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:group>
        <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
        <xs:group name="stage.content">
            <xs:sequence>
                <xs:group ref="ecc:stage.content"/>
                <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
                <!-- add site specific stage items here (optional) -->
                <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
            </xs:sequence>
        </xs:group>
        <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
        <xs:group name="additional.content">
            <xs:sequence>
                <xs:group ref="ecc:additional.content"/>
                <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
                <!-- add site specific additional items here (optional) -->
                <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
            </xs:sequence>
        </xs:group>
        <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    </xs:redefine>
