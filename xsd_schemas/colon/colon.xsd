    xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <xs:include schemaLocation="../framework/types.xsd"/>
    <xs:include schemaLocation="values.xsd"/>
    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <xs:redefine schemaLocation="../framework/common.xsd">
        <xs:attributeGroup name="generic.attrib">
            <xs:attributeGroup ref="ecc:generic.attrib"/>
            <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
            <!-- add any site-specific "universal" attributes here (optional) -->
            <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
        </xs:attributeGroup>
    </xs:redefine>
    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <xs:redefine schemaLocation="../framework/items.xsd">
        <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
        <xs:attributeGroup name="nodes.nodeGroup.result.sitespecific.attrib">
            <xs:attributeGroup ref="ecc:nodes.nodeGroup.result.sitespecific.attrib"/>
            <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
            <!-- add any site-specific nodeGroup result attributes here (optional) -->
            <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
        </xs:attributeGroup>
        <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
        <xs:attributeGroup name="tumor.focus.sitespecific.attrib">
            <xs:attributeGroup ref="ecc:tumor.focus.sitespecific.attrib"/>
            <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
            <!-- add any site-specific nodeGroup result attributes here (optional) -->
            <xs:attribute name="focality" type="ecc:focality.values"/>
            <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
        </xs:attributeGroup>
        <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    </xs:redefine>
    <xs:redefine schemaLocation="../framework/sections.xsd">
        <xs:attributeGroup name="resection.attrib">
            <xs:attributeGroup ref="ecc:resection.attrib"/>
            <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
            <!-- fill in the url of the official schemaLocation -->
            <xs:attribute fixed="http://purl.org/pathology/ecc/colon/schema.xsd"
                name="schemaLocation"/>
            <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
            <!-- fill in the version -->
            <xs:attribute fixed="0.9" name="version"/>
            <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
            <!-- fill in the url of the official transformation -->
            <xs:attribute fixed="http://purl.org/pathology/ecc/colon/ecc.xsl" name="transformation"
            />
        </xs:attributeGroup>
