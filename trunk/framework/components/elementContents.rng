	<!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
	<!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
	<div>
		<!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
		<a:documentation>-- nodes CONTENT --</a:documentation>
		<!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
		<define name="nodes.group.elements">
			<!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
			<a:documentation> </a:documentation>
			<!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
			<interleave>
				<optional>
					<ref name="common.specify"/>
				</optional>
				<group>
					<element name="count">
						<attribute name="status">
							<value>total</value>
						</attribute>
						<interleave>
							<grammar>
								<include href="&response;">
									<define name="value.attrib">
										<ref name="value.dimensionless.attrib"/>
									</define>
									<define name="value.numeric.result">
										<data type="nonNegativeInteger"/>
									</define>
								</include>
							</grammar>
							<ref name="common.elements"/>
						</interleave>
						<ref name="nodes.group.category.attrib"/>
					</element>
					<optional>
						<element name="count">
							<attribute name="status">
								<value>positive</value>
							</attribute>
							<interleave>
								<grammar>
									<include href="&response;">
										<define name="value.attrib">
											<ref name="value.dimensionless.attrib"/>
										</define>
										<define name="value.numeric.result">
											<data type="nonNegativeInteger"/>
										</define>
									</include>
								</grammar>
								<ref name="common.elements"/>
								<optional>
									<element name="largest">
										<ref name="common.sizeContent"/>
										<ref name="nodes.group.largest.attrib"/>
									</element>
								</optional>
								<optional>
									<element name="extracapsular">
										<interleave>
											<grammar>
												<include href="&response;">
													<define name="value.attrib">
														<ref name="value.text.attrib"/>
													</define>
													<define name="value.text.result">
														<parentRef name="common.extendedBoolean.values"/>
													</define>
												</include>
											</grammar>
											<ref name="common.elements"/>
										</interleave>
										<ref name="nodes.group.extracapsular.attrib"/>
									</element>
								</optional>
							</interleave>
							<ref name="nodes.group.category.attrib"/>
						</element>
					</optional>
					<optional>
						<element name="count">
							<attribute name="status">
								<value>regressed</value>
							</attribute>
							<interleave>
								<grammar>
									<include href="&response;">
										<define name="value.attrib">
											<ref name="value.dimensionless.attrib"/>
										</define>
										<define name="value.numeric.result">
											<data type="nonNegativeInteger"/>
										</define>
									</include>
								</grammar>
								<ref name="common.elements"/>
							</interleave>
							<ref name="nodes.group.category.attrib"/>
						</element>
					</optional>
					<optional>
						<element name="count">
							<attribute name="status">
								<value>specify</value>
							</attribute>
							<interleave>
								<grammar>
									<include href="&response;">
										<define name="value.attrib">
											<ref name="value.dimensionless.attrib"/>
										</define>
										<define name="value.numeric.result">
											<data type="nonNegativeInteger"/>
										</define>
									</include>
								</grammar>
								<ref name="common.elements"/>
								<element name="specify">
									<text/>
									<ref name="common.specify.attrib"/>
								</element>
							</interleave>
							<ref name="nodes.group.category.attrib"/>
						</element>
					</optional>
				</group>
			</interleave>
		</define>
		<!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
		<define combine="choice" name="nodes.group.location.values">
			<empty/>
		</define>
		<!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
		<define combine="interleave" name="nodes.siteSpecific.elements">
