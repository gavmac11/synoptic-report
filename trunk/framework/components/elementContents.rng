		<!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
		<define combine="interleave" name="margins.margin.elements">
			<interleave>
				<grammar>
					<include href="&response;">
						<define name="value.attrib">
							<ref name="value.text.attrib"/>
							<optional>
								<attribute name="closest">
									<parentRef name="common.extendedBoolean.values"/>
								</attribute>
							</optional>
						</define>
						<define name="value.text.result">
							<parentRef name="margins.margin.value.values"/>
						</define>
						<define name="value.extension.elements">
							<optional>
								<element name="distance">
									<parentRef name="length.content"/>
									<parentRef name="margins.margin.distance.attrib"/>
								</element>
							</optional>
						</define>
					</include>
				</grammar>
				<optional>
					<ref name="common.specify"/>
				</optional>
			</interleave>
		</define>
		<!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
		<define combine="interleave" name="margins.siteSpecific.elements">
			<empty/>
		</define>
