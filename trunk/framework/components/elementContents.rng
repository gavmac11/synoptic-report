		<define name="metastases.metastasis.elements">
			<interleave>
				<optional>
					<ref name="common.size"/>
				</optional>
				<!--<grammar>
          <include href="&response;">
            <define name="value.attrib">
              <ref name="value.text.attrib"/>
            </define>
            <define name="value.text.result">
              <parentRef name="common.extendedBoolean.values"/>
            </define>
            <define name="value.extension.elements">
              
            </define>
          </include>
        </grammar>-->
				<optional>
					<ref name="common.specify"/>
				</optional>
				<optional>
					<element name="howDiagnosed">
						<grammar>
							<include href="&response;">
								<define name="value.attrib">
									<ref name="value.text.attrib"/>
								</define>
								<define name="value.text.result">
									<parentRef name="metastases.metastasis.howDiagnosed.values"/>
								</define>
							</include>
						</grammar>
