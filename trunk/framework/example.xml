    <additional>
        <finding>
            <response value="unreported"/>
        </finding>
    </additional>
    <history>
        <clinicalFinding>
            <response value="unreported"/>
        </clinicalFinding>
        <priorTherapy>
            <response value="chemotherapy"/>
        </priorTherapy>
    </history>
    <extent>
        <contiguousExtension>
            <status of="unreported">
                <result for="invades">
                    <response value="positive"/>
                </result>
            </status>
        </contiguousExtension>
    </extent>
    <margin>
        <status of="unreported" on="left">
            <result for="invasive">
                <response closest="positive" value="negative"/>
            </result>
            <distance for="invasive">
                <response unit="mm" value="1"/>
            </distance>
            <result for="in-situ">
                <response closest="positive" value="negative"/>
            </result>
            <distance for="in-situ">
                <response unit="mm" value="1"/>
            </distance>
        </status>
        <status of="unreported">
            <result for="invasive">
                <response value="negative"/>
            </result>
            <distance for="invasive">
                <response unit="mm" value="2"/>
            </distance>
            <result for="in-situ">
                <response value="negative"/>
            </result>
            <distance for="in-situ">
                <response unit="mm" value="2"/>
            </distance>
        </status>
    </margin>
    <distant>
        <status of="bone">
            <result date="20090101" for="clinical history">
                <response value="specify">
                    <specify>something</specify>
                </response>
            </result>
            <size>
                <response unit="cm" value="3"/>
                <response unit="cm" value="2"/>
                <response unit="cm" value="1"/>
            </size>
        </status>
    </distant>
    <nodes>
        <status of="unreported">
            <result for="total">
                <response value="13"/>
            </result>
            <result for="negative">
                <response value="13"/>
            </result>
        </status>
        <status of="inapplicable" on="left">
            <result for="total">
                <response value="10"/>
            </result>
            <result for="negative">
                <response value="9"/>
            </result>
            <result for="positive">
                <response value="1"/>
            </result>
            <largestMetastasis>
                <response unit="cm" value="1"/>
                <response unit="cm" value="1"/>
                <response unit="cm" value="1"/>
            </largestMetastasis>
            <extracapsularExtension>
                <response value="unreported"/>
            </extracapsularExtension>
        </status>
    </nodes>
    <specimen>
        <procedure>
            <response value="unreported"/>
        </procedure>
        <size>
            <response unit="cm" value="1"/>
            <response unit="cm" value="1"/>
            <response unit="cm" value="1"/>
