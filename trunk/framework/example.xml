        </pN>
        <pM>
            <response value="unreported"/>
        </pM>
        <group>
            <response value="unreported"/>
        </group>
    </stage>
    <studies/>
    <tumor>
        <focus type="principal">
            <site>
                <response value="unreported"/>
            </site>
            <size>
                <response unit="mm" value="9"/>
            </size>
            <histologicType>
                <response value="unreported"/>
            </histologicType>
            <grade system="4-grade">
                <response value="4"/>
            </grade>
        </focus>
        <focus name="additional">
            <site>
                <response value="unreported"/>
            </site>
            <size>
                <response unit="cm" value="1"/>
            </size>
            <multiplicity>
                <response value="unreported"/>
            </multiplicity>
            <histologicType>
                <response value="unreported"/>
            </histologicType>
            <grade system="4-grade">
                <response value="3"/>
            </grade>
        </focus>
