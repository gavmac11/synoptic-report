<grammar datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes" xmlns="http://relaxng.org/ns/structure/1.0"
    xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0" xmlns:lung="&pert;lung" xmlns:pert="&pert;"
    xmlns:rng="http://relaxng.org/ns/structure/1.0">
    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <include href="../../common.rng">
        <a:documentation>Include the <synopsis/> element and associated base content.</a:documentation>
        <define combine="choice" name="schemaLocation">
            <a:documentation>Redefine the @schemaLocation to point to this schema.</a:documentation>
            <value>&repository;</value>
        </define>
        <define combine="choice" name="version">
            <a:documentation>Version of this schema</a:documentation>
            <value>&version;</value>
        </define>
    </include>
    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <include href="../../../method/cap-cancer/resection.rng">
        <a:documentation>Include the sectional architecture of a generic resection report. No structured content is defined.</a:documentation>
    </include>
    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <include href="&cap-cancer;clinical-content.rng">
        <a:documentation>Include the generic structured content of a Clinical section.</a:documentation>
    </include>
    <define combine="interleave" name="clinical-content">
        <a:documentation>Interleave new lung-specific additional content for the Clinical section.</a:documentation>
        <externalRef href="&add;clinical-smokingHistory.rng"/>
    </define>
    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <include href="&cap-cancer;specimen-content.rng">
        <a:documentation>Include the generic structured content of a Specimen section.</a:documentation>
        <define combine="choice" name="procedures">
            <a:documentation>Force lung-specific redefinition of the "sites" pattern of the generic Specimen section.</a:documentation>
            <externalRef href="&redefine;specimen-procedures-resection.rng"/>
        </define>
        <define combine="choice" name="sites">
            <a:documentation>Force lung-specific redefinition of the "sites" pattern of the generic Specimen section.</a:documentation>
            <externalRef href="&redefine;specimen-sites-resection.rng"/>
        </define>
    </include>
    <define combine="interleave" name="specimen-content" ns="&lung;">
        <a:documentation>Interleave new lung-specific additional content for the Specimen section.</a:documentation>
        <empty/>
    </define>
    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <include href="&cap-cancer;tumor-content.rng">
        <a:documentation>Include the generic structured content of a Tumor section.</a:documentation>
        <define name="histologicTypes">
            <a:documentation>Force lung-specific redefinition of the "histologicTypes" pattern of the generic Specimen section.</a:documentation>
            <externalRef href="&redefine;tumor-histologicTypes.rng"/>
        </define>
        <define name="grades">
            <a:documentation>Force lung-specific redefinition of the "grades" pattern of the generic Specimen section.</a:documentation>
            <externalRef href="&redefine;tumor-grades.rng"/>
        </define>
        <define name="gradingSystems">
            <a:documentation>Force lung-specific redefinition of the "gradingSystems" pattern of the generic Specimen section.</a:documentation>
            <externalRef href="&redefine;tumor-gradingSystems.rng"/>
        </define>
    </include>
    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <include href="&cap-cancer;extent-content.rng">
        <a:documentation>Include the generic structured content of an Extent section.</a:documentation>
    </include>
    <define combine="interleave" name="extent-content">
        <a:documentation>Interleave new lung-specific additional content for the Specimen section.</a:documentation>
        <interleave>
            <externalRef href="&add;extent-adjacentStructure.rng"/>
            <externalRef href="&add;extent-carina.rng"/>
            <externalRef href="&add;extent-mainstemBronchus.rng"/>
            <externalRef href="&add;extent-malignantEffusion.rng"/>
            <externalRef href="&add;extent-obstructivePneumonitis.rng"/>
            <externalRef href="&add;extent-visceralPleura.rng"/>
            <externalRef href="&add;extent-focality.rng"/>
        </interleave>
    </define>
    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <include href="&cap-cancer;margin-content.rng">
        <a:documentation>Include the generic structured content of a Margin section.</a:documentation>
        <define name="marginLocations">
            <a:documentation>Force lung-resection-specific redefinition of the "locations" pattern of the generic Margin section.</a:documentation>
            <externalRef href="&redefine;margin-locations-resection.rng">
                <a:documentation/>
            </externalRef>
        </define>
        <define name="marginStatuses">
            <a:documentation>Force lung-resection-specific redefinition of the "statuses" pattern of the generic Margin section.</a:documentation>
            <externalRef href="&redefine;margin-statuses.rng"/>
        </define>
    </include>
    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <include href="&cap-cancer;accessory-content.rng">
        <a:documentation>Include the generic structured content of an Accessory section.</a:documentation>
        <define name="treatmentEffectValues">
            <externalRef href="&redefine;accessory-treatmentEffectValues.rng"/>
        </define>
    </include>
    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <include href="&cap-cancer;nodes-content.rng">
        <define name="nodeGroups">
            <externalRef href="&redefine;nodes-nodeGroups.rng"/>
        </define>
    </include>
    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <include href="&cap-cancer;mets-content.rng"/>
    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <include href="&cap-cancer;studies-content.rng"/>
    <define name="studies-content">
        <externalRef href="&add;studies-egfr.rng"/>
    </define>
    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <include href="&cap-cancer;stage-content.rng"/>
    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <include href="&cap-cancer;additional-content.rng">
        <define name="findings">
            <externalRef href="&redefine;additional-findings.rng"/>
        </define>
    </include>
    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
</grammar>
