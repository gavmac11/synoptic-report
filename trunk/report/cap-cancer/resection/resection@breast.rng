<!ENTITY cap-cancer "../../../section/cap-cancer/">
<!ENTITY method "../../../method/cap-cancer/">
<!ENTITY report "../../">
<!ENTITY version "0.1">
<!ENTITY common "../../../common/">
<!ENTITY repository "http://purl.oclc.org/medical/reporting/report/cap-cancer/resection/resection@breast.rng">
]>
