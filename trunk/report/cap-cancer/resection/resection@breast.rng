<grammar datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes"
    xmlns="http://relaxng.org/ns/structure/1.0"
    xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
    xmlns:breast="&pert;breast/" xmlns:pert="&pert;">
    <include href="&report;common.rng">
        <define combine="choice" name="schemaLocation">
            <value>&repository;</value>
        </define>
        <define combine="choice" name="version">
            <value>&version;</value>
        </define>
    </include>
    <include href="&method;resection.rng"/>
    <include href="&cap-cancer;clinical-content.rng">
        <define name="clinicalFindingValues">
            <externalRef href="&redefine;clinical-clinicalFindingValues.rng"
            />
        </define>
        <define name="radiologicFindingValues">
            <externalRef
                href="&redefine;clinical-radiologicFindingValues.rng"/>
        </define>
    </include>
    <a:documentation>Specimen section</a:documentation>
    <include href="&cap-cancer;specimen-content.rng">
        <a:documentation>Site-specific content redefines for specimen section</a:documentation>
        <define combine="choice" name="procedures">
            <externalRef href="&redefine;specimen-procedures.rng"/>
        </define>
        <define combine="choice" name="sites">
            <externalRef href="&redefine;specimen-sites.rng"/>
        </define>
        <define name="integrityValues">
            <externalRef href="&redefine;specimen-integrityValues.rng"/>
        </define>
    </include>
    <a:documentation>Site-specific new content for specimen section</a:documentation>
    <define combine="interleave" name="site-content">
        <externalRef href="&common;laterality.rng"/>
    </define>
    <a:documentation>Tumor section</a:documentation>
    <include href="&cap-cancer;tumor-content.rng">
        <a:documentation>Site-specific content redefines for tumor section</a:documentation>
        <define name="histologicTypes">
            <externalRef href="&redefine;tumor-histologicTypes.rng"/>
        </define>
        <define name="gradeContent">
            <externalRef href="&redefine;tumor-gradeContent.rng"/>
        </define>
        <define name="gradingSystems">
            <externalRef href="&redefine;tumor-gradingSystems.rng"/>
        </define>
    </include>
    <a:documentation>Site-specific new content for tumor section</a:documentation>
    <include href="&cap-cancer;extent-content.rng"/>
    <define combine="interleave" name="extent-content">
        <externalRef href="&add;extent-invasion.rng"/>
    </define>
    <include href="&cap-cancer;margin-content.rng">
        <define name="marginLocations">
            <externalRef href="&redefine;margin-locations-resection.rng"/>
        </define>
        <define name="marginStatuses">
            <externalRef href="&redefine;margin-statuses.rng"/>
        </define>
    </include>
    <define combine="interleave" name="margin-content">
        <attribute name="for">
            <choice xmlns="http://relaxng.org/ns/structure/1.0">
                <value>carcinoma-in-situ</value>
                <value>invasive carcinoma</value>
            </choice>
        </attribute>
    </define>
    <include href="&cap-cancer;accessory-content.rng">
        <define name="treatmentEffectValues">
            <externalRef
                href="&redefine;accessory-treatmentEffectValues.rng"/>
        </define>
    </include>
    <define combine="interleave" name="accessory-content">
        <externalRef href="&add;accessory-dermalLymphatic.rng"/>
        <externalRef href="&add;accessory-microcalcification.rng"/>
    </define>
    <include href="&cap-cancer;nodes-content.rng">
        <define name="nodeGroups">
            <externalRef href="&redefine;nodes-nodeGroups.rng"/>
        </define>
        <define name="nodeStatuses">
            <externalRef href="&redefine;nodes-statuses.rng"/>
        </define>
    </include>
    <include href="&cap-cancer;mets-content.rng"/>
    <include href="&cap-cancer;studies-content.rng"/>
    <define name="studies-content">
        <interleave>
            <externalRef href="&add;studies-estrogenReceptor.rng"/>
            <externalRef href="&add;studies-progesteroneReceptor.rng"/>
            <externalRef href="&add;studies-her2neu.rng"/>
        </interleave>
    </define>
    <include href="&cap-cancer;stage-content.rng">
        <define name="T-values">
            <externalRef href="&redefine;stage-T-values.rng"/>
        </define>
        <define name="N-values">
            <externalRef href="&redefine;stage-N-values.rng"/>
        </define>
        <define name="M-values">
            <externalRef href="&redefine;stage-M-values.rng"/>
        </define>
    </include>
    <include href="&cap-cancer;additional-content.rng">
        <define name="findings">
            <externalRef href="&redefine;additional-findings.rng"/>
        </define>
    </include>
</grammar>
